{
  "name": "llamalize",
  "version": "0.15.0",
  "description": "Transform any human readable alphanumeric string divided by certain characters into camel form.",
  "main": "llamalize.js",
  "support": "llamalize.support.js",
  "deploy": "llamalize.deploy.js",
  "browser": "llamalize.deploy.js",
