  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/volkovasystems/llamalize/issues"
  },
  "homepage": "https://github.com/volkovasystems/llamalize#readme",
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "falzy": "^0.20.0",
    "protype": "^0.22.0",
    "titlelize": "^0.16.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
